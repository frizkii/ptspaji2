<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#D6D6D6"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WhereAdmin">

    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.cardview.widget.CardView
            android:layout_gravity="center_horizontal"
            android:id="@+id/cardtouser"
            android:background="@color/basecolor"
            app:cardCornerRadius="@dimen/_8sdp"
            android:layout_centerHorizontal="true"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_90sdp">
            <ImageView
                android:id="@+id/icontouser"
                android:layout_gravity="center"
                android:background="@drawable/userlist"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_gravity="center_horizontal"
            android:id="@+id/cardtoitem"
            android:background="@color/basecolor"
            app:cardCornerRadius="@dimen/_8sdp"
            android:layout_centerHorizontal="true"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_90sdp">
            <ImageView
                android:id="@+id/icontoitem"
                android:layout_gravity="center"
                android:background="@drawable/bicyclelist"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_gravity="center_horizontal"
            android:id="@+id/cardtotransaction"
            android:background="@color/basecolor"
            app:cardCornerRadius="@dimen/_8sdp"
            android:layout_centerHorizontal="true"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_90sdp">
            <ImageView

                android:id="@+id/icontotransaction"
                android:layout_gravity="center"
                android:background="@drawable/bookinglist"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"/>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
